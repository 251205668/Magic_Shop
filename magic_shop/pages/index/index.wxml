<view>
<image class="toptheme" src="{{topThemeImg.entrance_img}}"></image>
<swiper class="swiper" circular="{{true}}" indicator-active-color="#157658" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{5000}}" duration="{{500}}">
  <block wx:for="{{banner.items}}" wx:key="{{index}}" :key="{{index}}">
    <swiper-item>
      <image class="swiper" src="{{item.img}}" />
    </swiper-item>
  </block>
</swiper>
<H-grid grid="{{grid}}"></H-grid>
<image class="activity" src="{{activity.entrance_img}}" />
<H-scroll theme="{{themeB}}" spu-list="{{ThemeBSpuList}}"></H-scroll>
<image class="themeC" src="{{themeC.entrance_img}}" />
<H-hot-list hotlist="{{hotList}}"></H-hot-list>
<image class="themeD" src="{{themeD.entrance_img}}" />
<H-recommend-list></H-recommend-list>
</view>
